{
  "defaultReadmeFrontmatter": {
    "deprecated": false,
    "hidden": false,
    "metadata": { "robots": "index" }
  },
  "titleFrom": ["sidebar_label", "h1"],
  "replacements": [
    {
      "description": "Transform <details> to a ReadMe Accordion MDX component",
      "whenHtmlTag": "details",
      "replaceWith": "Accordion",
      "propsMap": { "open": true },
      "innerToChildren": true
    }
  ]
}
